<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.professionalandroid.apps.ssack_3re">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SSack_3Re">
        <activity android:name=".Memo_update"></activity>
        <activity android:name=".AlarmSet" ></activity>
        <activity android:name=".Memo_add" />
        <activity android:name=".Vinyl" />
        <activity android:name=".sugetion" />
        <activity android:name=".Stiropom" />
        <activity android:name=".Search" />
        <activity android:name=".Plastic" />
        <activity android:name=".Paper" />
        <activity android:name=".Memo" />
        <activity android:name=".AlarmActivity" />
        <activity android:name=".Information" />
        <activity android:name=".Glass" />
        <activity android:name=".Can" />
        <activity android:name=".Calendar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".AlarmReceiver"
            android:exported="false"
            android:enabled="true"/>
        <service
            android:name=".AlarmService"
            android:enabled="true"/>

    </application>

</manifest>