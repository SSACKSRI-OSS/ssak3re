<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/paper"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Paper">

    <LinearLayout
        android:id="@+id/paper_view_1"
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="1dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="16dp"
                    android:rotation="90"
                    android:visibility="visible"
                    app:srcCompat="@drawable/paper" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="249dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="46dp"
                    android:layout_marginLeft="46dp"
                    android:layout_marginTop="-30dp"
                    android:fontFamily="sans-serif-black"
                    android:text="종이"
                    android:textColor="#000000"
                    android:textSize="23sp" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="6dp"
                    android:layout_marginRight="6dp"
                    android:text="l 신문지"
                    android:textColor="#333030"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="297dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="130dp"
                    android:layout_marginRight="130dp"
                    android:text="- 물기에 젖지 않도록 하고 반듯하게 펴서 차곡차곡 쌓은 후 묶어서 배출합니다.\n- 비닐 코팅된 광고지, 비닐류, 기타 오물이 섞이지 않도록 합니다."
                    android:textSize="12sp"
                    tools:visibility="visible" />

                <ImageView
                    android:id="@+id/imageView19"
                    android:layout_width="75dp"
                    android:layout_height="92dp"
                    android:layout_marginStart="316dp"
                    android:layout_marginLeft="316dp"
                    android:layout_marginTop="-90dp"
                    android:layout_marginEnd="0dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="10000"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView8"
                    app:srcCompat="@drawable/newspaper" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="7dp"
                    android:text="I 책자 · 노트 등"
                    android:textColor="#333030" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="296dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="130dp"
                    android:layout_marginRight="130dp"
                    android:text="- 비닐 코팅된 표지, 공책의 스프링 등은 제거한 후 배출합니다."
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="262dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginLeft="40dp"
                    android:layout_marginTop="-15dp"
                    android:text="*비닐포장지는 제외"
                    android:textColor="#F61F1F"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/imageView20"
                    android:layout_width="237dp"
                    android:layout_height="114dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginLeft="20dp"
                    app:srcCompat="@drawable/spring" />

                <ImageView
                    android:id="@+id/imageView21"
                    android:layout_width="84dp"
                    android:layout_height="97dp"
                    android:layout_marginStart="270dp"
                    android:layout_marginLeft="270dp"
                    android:layout_marginTop="-100dp"
                    app:srcCompat="@drawable/books" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="7dp"
                    android:text="I 상자류 (골판지 상자 등)"
                    android:textColor="#333030" />

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:text="- 비닐코팅 부분, 상자에 붙어있는 테이프 · 철핀 등을 제거한 후 압착하여 운반이 용이하도록 묶어서 배출합니다. "
                    android:textSize="12sp" />

                <ImageView
                    android:id="@+id/imageView22"
                    android:layout_width="333dp"
                    android:layout_height="107dp"
                    android:layout_marginStart="35dp"
                    android:layout_marginLeft="35dp"
                    app:srcCompat="@drawable/box_sort" />

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="-----------------------------------------------------------------------------------------------------------" />

                <ImageView
                    android:id="@+id/imageView23"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="10dp"
                    android:rotation="90"
                    app:srcCompat="@drawable/paperpack" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="46dp"
                    android:layout_marginLeft="46dp"
                    android:layout_marginTop="-30dp"
                    android:fontFamily="sans-serif-black"
                    android:text="종이팩 · 종이컵"
                    android:textColor="#000000"
                    android:textSize="23sp" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="6dp"
                    android:layout_marginRight="6dp"
                    android:text="l 종이팩 (살균팩, 멸균팩)"
                    android:textColor="#333030"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="380dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="130dp"
                    android:layout_marginRight="130dp"
                    android:text="- 내용물을 비우고 물로 한번 헹군 후 압착하여 봉투에 넣거나 한데 묶어서 배출합니다.\n- 빨대, 비닐 등 종이팩과 다른 재질은 제거한 후 배출합니다.\n- 일반 종이류와 혼합되지 않게 종이팩 전용수거함에 배출합니다.\n- 종이팩 전용수거함이 없는 경우에는 종이류와 구분할 수 있도록 가급적 끈 등으로 묶어 종이류 수거함으로 배출합니다."
                    android:textSize="12sp"
                    tools:visibility="visible" />

                <ImageView
                    android:id="@+id/imageView24"
                    android:layout_width="334dp"
                    android:layout_height="136dp"
                    android:layout_marginStart="29dp"
                    android:layout_marginLeft="29dp"
                    app:srcCompat="@drawable/milkpack_sort" />

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="6dp"
                    android:layout_marginRight="6dp"
                    android:text="l 종이컵"
                    android:textColor="#333030"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="291dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="130dp"
                    android:layout_marginRight="130dp"
                    android:text="- 내용물을 비우고 물로 한번 헹군 후 압착하여 봉투에 넣거나 한데 묶어서 배출합니다.\n\n\n\n\n\n\n\n\n\n\n"
                    android:textSize="12sp"
                    tools:visibility="visible" />

                <ImageView
                    android:id="@+id/imageView25"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="315dp"
                    android:layout_marginLeft="315dp"
                    android:layout_marginTop="-200dp"
                    app:srcCompat="@drawable/papercup" />

            </LinearLayout>
        </ScrollView>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>